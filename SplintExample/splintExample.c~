#include <stdio.h>
int main()
{
    char c;
    while (c != 'x');
    {
        c = getchar();
        if (c == 'x')
            return 0;
        switch (c) {
        case '\n':break
        case '\r':break
            printf("Newline\n");
        default:break
            printf("%c",c);
        }
    }
    return 0;
}

